{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": [2, { "js": "always", "json": "always" }],
    "linebreak-style": 0,
    "newline-before-return": "error"
  }
}
